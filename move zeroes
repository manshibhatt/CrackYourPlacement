class Solution {
    public void moveZeroes(int[] arr) {
          int me=0;
for(int i=0;i<arr.length;i++){
    if(arr[i]!=0){ 
        arr[me]=arr[i];
        me++;
    }
} 
for(int i=me;i<arr.length;i++)
arr[i]=0; 

 for(int i=0;i<arr.length;i++)
         System.out.print(arr[i]+" "); 
        
    }
}
